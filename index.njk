---
layout: base.njk
title: Home
---

<main class="main-content">
  <ul class="blog-links">
    {% for post in collections.all | reverse %}
      {% if "/posts/" in post.inputPath %}
        <li>
          <a href="{{ post.url }}">{{ post.data.title }}</a>
        </li>
      {% endif %}
    {% endfor %}
  </ul>
</main>

